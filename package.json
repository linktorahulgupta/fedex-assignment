{
  "name": "tnt-assignment",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "merge": "mochawesome-merge mochawesome-report/*.html -o mochawesome-report/searchTestsReport.html",
    "generate_mochawesome_report": "marge mochawesome-report/searchTestsReport.html -f output -o mochawesome-report",
    "final_report": "npm run merge && npm run generate_mochawesome_report",
    "cyTest": "node_modules/.bin/cypress run  --spec cypress/integration/e2e/*.js --record --key 78804739-dd1f-40ce-a520-40d4e803dab0 && npm run final_report",
    "headTest": "npm run cyTest -- --headed",
    "chromeTest": "npm run cyTest -- --browser chrome"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.4",
    "@angular/common": "7.2.4",
    "@angular/compiler": "7.2.4",
    "@angular/core": "7.2.4",
    "@angular/forms": "7.2.4",
    "@angular/http": "7.2.4",
    "@angular/platform-browser": "7.2.4",
    "@angular/platform-browser-dynamic": "7.2.4",
    "@angular/router": "7.2.4",
    "core-js": "^2.6.11",
    "cypress": "^9.3.1",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "tsify": "^5.0.4",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "7.2.4",
    "@angular/language-service": "7.2.4",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^6.14.13",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^9.1.4",
    "mochawesome": "^7.0.1",
